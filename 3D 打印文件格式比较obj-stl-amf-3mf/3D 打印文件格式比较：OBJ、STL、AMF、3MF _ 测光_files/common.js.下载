function comparer(index){return function(a,b){var valA=getCellValue(a,index).replace('€',''),valB=getCellValue(b,index).replace('€','');if($.isNumeric(valA.charAt(0))){valA=valA.replace(/ –.*$/,"")}
if($.isNumeric(valB.charAt(0))){valB=valB.replace(/ –.*$/,"")}
return jQuery.isNumeric(valA)&&jQuery.isNumeric(valB)?valA-valB:valA.toString().localeCompare(valB)}}
function getCellValue(row,index){return jQuery(row).children('td').eq(index).text()}